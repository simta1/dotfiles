def_key "j"
    scroll_down
def_key "k"
    scroll_up
def_key "h"
    previous_column
def_key "l"
    next_column

def_key "g"
    move_home
def_key "G"
    move_end

def_key "space"
    pause
def_key "enter"
    play_item

def_key ">"
    next
def_key "<"
    previous

def_key "delete"
    delete_playlist_items

def_key "y"
    select_item
def_key "p"
    add_selected_items
